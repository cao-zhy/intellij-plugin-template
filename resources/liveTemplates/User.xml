<templateSet group="User">
  <template name="sop" value="System.out.print($END$);" description="Prints a string" toReformat="true"
            toShortenFQNames="true">
    <context>
      <option name="JAVA_STATEMENT" value="true"/>
    </context>
  </template>
  <template name="svu" value="private static final long serialVersionUID = 1L;" description="Defines serialVersionUID"
            toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JAVA_DECLARATION" value="true"/>
    </context>
  </template>
  <template name="utf" value="&lt;project.build.sourceEncoding&gt;UTF-8&lt;/project.build.sourceEncoding&gt;"
            description="Set encoding to UTF-8" toReformat="true" toShortenFQNames="false">
    <context>
      <option name="MAVEN" value="true"/>
    </context>
  </template>
  <template name="sbd"
            value="&lt;dependency&gt;&#10;  &lt;groupId&gt;org.springframework.boot&lt;/groupId&gt;&#10;  &lt;artifactId&gt;spring-boot-dependencies&lt;/artifactId&gt;&#10;  &lt;version&gt;${springboot.version}&lt;/version&gt;&#10;  &lt;scope&gt;import&lt;/scope&gt;&#10;  &lt;type&gt;pom&lt;/type&gt;&#10;&lt;/dependency&gt;"
            description="Add spring boot dependencies" toReformat="true" toShortenFQNames="false">
    <context>
      <option name="MAVEN" value="true"/>
    </context>
  </template>
</templateSet>